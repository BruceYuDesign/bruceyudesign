{
    "functions": {
        "source": ".output/server",
        "runtime": "nodejs18",
        "headers": [
            {
                "key": "Access-Control-Allow-Origin",
                "value": "https://bruce-yu-design.web.app"
            },
            {
                "key": "Access-Control-Allow-Credentials",
                "value": "true"
            }
        ]
    },
    "hosting": [
        {
            "site": "bruce-yu-design",
            "public": ".output/public",
            "cleanUrls": true,
            "rewrites": [{
                "source": "**",
                "function": "server"
            }]
        }
    ]
}